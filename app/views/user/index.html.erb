<h1> PhotoSite Website...</h1>
<h2> These are the users</h2>
<table style="font-size: 20px; text-align: center; font-family: Arial; text-align-all: center">
  <tr>
    <th>User Id </th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% for user in @users_all %>
  <tr>
    <td><%=user.id %></td>
    <td> <%=link_to user.first_name,{:action => "picture", :id => user.id} %></td>
    <td><%=user.last_name %> </td>
  </tr>
  <%end %>
</table>